{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "src/generated/**"]
    },
    "@skillrecordings/skill-api#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "lib/**",
        "css/**",
        "next/**",
        "client/**",
        "core/**",
        "index.d.ts",
        "index.js"
      ]
    },
    "@skillrecordings/config#build": {
      "dependsOn": [
        "^build",
        "$CONVERTKIT_API_SECRET",
        "$NEXT_PUBLIC_CONVERTKIT_TOKEN",
        "$NEXT_PUBLIC_CONVERTKIT_SIGNUP_FORM"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "postbuild": {
      "dependsOn": ["build"],
      "outputs": ["public/sitemap.xml", "public/robots.txt"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"]
    },
    "lint": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "integration": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
