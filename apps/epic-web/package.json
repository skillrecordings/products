{
  "name": "epic-web",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "pnpm -r exec -- rm -rf .turbo node_modules",
    "build": "remix build",
    "build:css": "tailwindcss -m -i app/styles/global.css -o app/tailwind.css",
    "dev": "concurrently \"remix dev\" \"pnpm dev:css\"",
    "dev:css": "tailwindcss -w -i app/styles/global.css -o app/tailwind.css"
  },
  "dependencies": {
    "@portabletext/react": "^1.0.6",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@remix-run/vercel": "^1.6.8",
    "@sanity/client": "^3.3.6",
    "@skillrecordings/convertkit-sdk": "workspace:*",
    "@vercel/node": "^2.4.4",
    "classnames": "^2.3.1",
    "framer-motion": "^6.4.1",
    "groq": "^2.15.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.3.2",
    "starfield-react": "3.1.0-dev-5",
    "tailwind-fluid-typography": "^1.3.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@portabletext/types": "^1.0.3",
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "@tailwindcss/typography": "^0.5.2",
    "@types/lodash": "^4.14.179",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.2.2",
    "eslint": "^8.20.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4"
  }
}
