{
  "exclude": [
    "node_modules"
  ],
  "extends": "@skillrecordings/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": "./src/",
    "rootDir": "src",
    "outDir": "dist",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "src",
    "next-env.d.ts",
    "process.d.ts",
    ".next/types/**/*.ts"
  ]
}
