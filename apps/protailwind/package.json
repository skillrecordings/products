{
  "name": "protailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production next build",
    "postbuild": "NODE_ENV=production next-sitemap",
    "build:dev": "pnpm -w build --filter protailwind",
    "clean": "rm -rf node_modules && pnpm -r exec -- rm -rf .turbo node_modules .next dist",
    "db:generate": "pnpm -w generate",
    "db:push": "prisma db push",
    "db:start": "docker compose up -d",
    "db:studio": "prisma studio",
    "dev": "next dev -p 3017",
    "dev:setup": "vercel env pull .env.local && sed -i.bak '/^VERCEL/d' .env.local",
    "dev:stripe": "stripe listen --forward-to localhost:3017/api/skill/webhook/stripe",
    "dev:sanity": "sanity dev",
    "format": "prettier --write ./src/**/*",
    "integration": "cypress run",
    "lint": "TIMING=1 eslint src/**/*.{ts,tsx} --fix && tsc --noEmit",
    "start": "next start -p 3017",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,json,md,mdx,ts,tsx}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,md,mdx,js}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "preset": "@skillrecordings/scripts/jest/next"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^1.3.0",
