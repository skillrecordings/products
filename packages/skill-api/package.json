{
  "name": "@skillrecordings/skill-api",
  "version": "0.0.1",
  "description": "",
  "license": "ISC",
  "author": "",
  "exports": {
    ".": "./index.js",
    "./client": "./client/index.js",
    "./core": "./core/index.js",
    "./next": "./next/index.js",
    "./lib": "./lib/index.js"
  },
  "main": "index.js",
  "files": [
    "lib",
    "css",
    "react",
    "next",
    "client",
    "core",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:js",
    "build:css": "postcss --config config/postcss.config.js src/**/*.css --base src --dir . && node config/wrap-css.js",
    "build:js": "pnpm clean && tsc && babel --config-file ./config/babel.config.js src --out-dir . --extensions \".tsx,.ts,.js,.jsx\"",
    "clean": "rm -rf client core lib next index.d.ts index.js",
    "lint": "eslint src config",
    "prepublishOnly": "pnpm build",
    "setup": "pnpm generate-providers",
    "test": "jest",
    "watch:css": "postcss --config config/postcss.config.js --watch src/**/*.css --base src --dir ."
  },
  "eslintIgnore": [
    "./*.d.ts",
    "**/tests",
    "**/__tests__"
  ],
  "jest": {
    "preset": "@skillrecordings/scripts/jest/node",
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "preact": "^10.7.2",
    "preact-render-to-string": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-optional-catch-binding": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@prisma/client": "^3.15.2",
    "@skillrecordings/scripts": "workspace:*",
    "@skillrecordings/tsconfig": "workspace:*",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/nodemailer": "^6.4.4",
    "@types/oauth": "^0.9.1",
    "autoprefixer": "^10.4.2",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-preset-preact": "^2.0.0",
    "cssnano": "^5.0.12",
    "eslint": "^8.19.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "msw": "^0.35.0",
    "next": "^12.1.6",
    "next-auth": "=4.2.1",
    "nodemailer": "^6.7.2",
    "nodemailer-postmark-transport": "^5.2.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "next-auth": "=4.2.1",
    "nodemailer": "^6.6.5",
    "nodemailer-postmark-transport": "^5.2.1",
    "react": "^17.0.2 || ^18",
    "react-dom": "^17.0.2 || ^18"
  },
  "engines": {
    "node": "^12.19.0 || ^14.19.0 || ^16.13.0"
  }
}
