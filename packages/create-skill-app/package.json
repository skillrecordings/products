{
  "name": "create-skill-app",
  "version": "12.2.8",
  "keywords": [
    "react",
    "next",
    "next.js"
  ],
  "description": "Create Next.js-powered React apps for Skill Recordings produces with one command",
  "repository": {
    "type": "git",
    "url": "https://github.com/skillrecordings/products",
    "directory": "packages/create-skill-app"
  },
  "author": "Skill Recordings Team <team@skillrecordings.com>",
  "license": "MIT",
  "bin": {
    "create-skill-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm prerelease && ncc build ./index.ts -w -o dist/",
    "clean": "rimraf ./dist/ && rimraf ./node_modules/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublishOnly": "cd ../../ && turbo run build",
    "build": "pnpm release"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/cross-spawn": "6.0.0",
    "@types/node": "^12.6.8",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.34.0",
    "async-retry": "1.3.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "prompts": "2.1.0",
    "rimraf": "3.0.2",
    "tar": "4.4.10",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  }
}
