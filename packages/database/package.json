{
  "name": "@skillrecordings/database",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "db:push": "prisma db push",
    "dev": "tsup --watch",
    "format": "prisma format",
    "generate": "DATABASE_URL=\"mysql://root@127.0.0.1:3306/turborepo\" prisma generate",
    "lint": "eslint ./src --fix",
    "prebuild": "pnpm run generate",
    "validate": "DATABASE_URL=\"mysql://root@127.0.0.1:3306/turborepo\" npx prisma validate",
    "predev": "pnpm run generate"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@skillrecordings/tsconfig": "workspace:*",
    "@types/node": "^18.6.1",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.12.0",
    "eslint-config-custom": "workspace:*",
    "i": "^0.3.7",
    "jest-mock-extended": "^2.0.7",
    "prisma": "^4.5.0",
    "rimraf": "^3.0.2",
    "tsup": "^5.11.13",
    "tsx": "^3.7.1",
    "typescript": "^4.8.3"
  }
}
